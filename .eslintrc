{
  "parser": "babel-eslint",
  "extends": [ "airbnb-base" ],
  "rules": {
    "arrow-body-style": 0,
    "max-len": 0,
    "no-param-reassign": 0,
    "no-bitwise": 0,
    "no-console": 0,
    "no-empty": 0,
    "no-plusplus": 0,
    "no-underscore-dangle": 0,
    "class-methods-use-this": 0,
    "consistent-return": 0,
    "radix": 0,
    "prefer-const": 0
  }
}
